{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "linebreak-style": ["off"],
        "indent": ["error", 4],
        "comma-dangle": "off",
        "prefer-destructuring": "off",
        "no-underscore-dangle": "off"
    }
}
